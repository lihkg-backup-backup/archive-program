維 #FergusLeung<br />\n#香港隊勁揪<br />\n<br />\n3<br />\n0<br />\n 3"},{"pid":"2f8d97e945fc52331157aa2ce3df153a85fa5748","tid":1782366,"uid":207462,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-14T02:00:47.000Z","msg":"推"},{"pid":"ce8551d58eb99105af0a32c421eff4af22ee700a","tid":1782366,"uid":85327,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-14T03:01:41.000Z","msg":"此回覆已被刪除"},{"pid":"2893d85f5e1e52aaa016b88dc84e105329685b2d","tid":1782366,"uid":57677,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-14T05:06:26.000Z","msg":"<img src=\"/assets/faces/normal/adore.gif\" class=\"hkgmoji\" /><img src=\"/assets/faces/normal/adore.gif\" class=\"hkgmoji\" />"},{"pid":"0a3b5e765f41861b73658a521cac95e47fefcd38","tid":1782366,"uid":52090,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-14T06:57:21.000Z","msg":"<img src=\"/assets/faces/normal/angel.gif\" class=\"hkgmoji\" />"},{"pid":"3d640dee4167b42af9969fca492da9bb98ccb0c6","tid":1782366,"uid":62270,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-14T06:58:13.000Z","msg":"冇時間啦25號截止，極度要人幫手!<br />\n<a href=\"https://lih.kg/1782422\" data-auto-link target=\"_blank\">https://lih.kg/1782422</a><br />\n- 分享自 LIHKG 討論區"},{"pid":"575c4cc9c22663b22f2e019cf48d8719db78a7fd","tid":1782366,"uid":235535,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-14T06:58:29.000Z","msg":"推"},{"pid":"aef0fa3e21d9f6c42a5f4600baef72943525f50f","tid":1782366,"uid":235535,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-14T06:58:43.000Z","msg":"推"},{"pid":"73a58b663d60294803c3ea92474b350afed871e2","tid":1782366,"uid":74182,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-14T07:00:26.000Z","msg":"推"},{"pid":"034f9a3c2545cbd014baf563d89117da8f9ea28f","tid":1782366,"uid":12824,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-14T07:02:40.000Z","msg":"<img src=\"/assets/faces/normal/kill.gif\" class=\"hkgmoji\" />"},{"pid":"49