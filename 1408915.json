gers"},{"pid":"7a6a8fbbfd0c1c07b336ddf9ca7544bf9e993adb","tid":1408915,"uid":156405,"like":0,"dislike":0,"score":0,"quote":{"pid":"6e8b4dbdd3c602ae6ace1ef148d5c72b4ab3e1aa","tid":1408915,"uid":77042,"like":0,"dislike":0,"score":0,"quote":{"pid":"de1fe9c6c94e7ceecda3dc69ee04c5f24c8437f0","tid":1408915,"uid":156405,"like":0,"dislike":0,"score":0,"citedBy":1,"replyTime":"2019-08-04T18:51:58.000Z","msg":"<img src=\"/assets/faces/normal/shocking.gif\" class=\"hkgmoji\" />"},"citedBy":1,"replyTime":"2019-08-04T18:54:07.000Z","msg":"漏左。sor"},"citedBy":1,"replyTime":"2019-08-04T18:57:46.000Z","msg":"<img src=\"/assets/faces/normal/fire.gif\" class=\"hkgmoji\" />"},{"pid":"635687fc0e5ff27934332d927ecf5547f9329f70","tid":1408915,"uid":77042,"like":0,"dislike":0,"score":0,"quote":{"pid":"7a6a8fbbfd0c1c07b336ddf9ca7544bf9e993adb","tid":1408915,"uid":156405,"like":0,"dislike":0,"score":0,"quote":{"pid":"6e8b4dbdd3c602ae6ace1ef148d5c72b4ab3e1aa","tid":1408915,"uid":77042,"like":0,"dislike":0,"score":0,"quote":{"pid":"de1fe9c6c94e7ceecda3dc69ee04c5f24c8437f0","tid":1408915,"uid":156405,"like":0,"dislike":0,"score":0,"citedBy":1,"replyTime":"2019-08-04T18:51:58.000Z","msg":"<img src=\"/assets/faces/normal/shocking.gif\" class=\"hkgmoji\" />"},"citedBy":1,"replyTime":"2019-08-04T18:54:07.000Z","msg":"漏左。sor"},"citedBy":1,"replyTime":"2019-08-04T18:57:46.000Z","msg":"<img src=\"/assets/faces/normal/fire.gif\" class=\"hkgmoji\" />"},"citedBy":1,"replyTime":"2019-08-04T19:03:36.000Z","msg":"太多地點 人可能會太少 同埋主要塞死港鐵站 然後其餘主要道路"},{"pid":"cdefeaab474f6524def3e5e85de35c43d4107215","tid":1408915,"uid":156405,"like":0,"dislike":0,"score":0,"quote":{"pid":"635687fc0e5ff27934332d927ecf5547f9329f70","tid":1408915,"uid":77042,"like":0,"dislike":0,"score":0,"quote":{"pid":"7a6a8fbbfd0c1c07b336ddf9ca7544bf9e993adb","tid":1408915,"uid":156405,"like":0,"dislike":0,"score":0,"quote":{"pid":"6e8b4dbdd3c602ae6ace1ef148d5c72b4ab3e1aa","tid":1408915,"uid":77042,"like":0,"dislike":0,"score":0,"citedBy":1,"repl