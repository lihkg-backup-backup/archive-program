span></span>"},{"pid":"a7300ea89350c7365fb2270b6df25dfbe0198cb6","tid":1703627,"uid":315724,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-10T17:59:43.000Z","msg":"推"},{"pid":"a120aec6f629f037d3cf789c3998b02d592b912c","tid":1703627,"uid":315724,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-10T18:00:04.000Z","msg":"多一個好多"},{"pid":"3791f72a6426251cc1fcc37c1266e7c64fc38b40","tid":1703627,"uid":315724,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-10T18:00:22.000Z","msg":"推行動post"},{"pid":"d0576cb0614d48fef665057f0f3dfa63914e93d7","tid":1703627,"uid":315724,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-10T18:10:00.000Z","msg":"行動post"},{"pid":"acce8dbd6283cbf18dfce301e4e27916ac33c440","tid":1703627,"uid":315724,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-10T18:10:19.000Z","msg":"Push"},{"pid":"1c4af7bf954cad3ba9850adc3a05cd38a904cc8f","tid":1703627,"uid":315724,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-10T18:10:37.000Z","msg":"Push"},{"pid":"2dc36a958c3314c15b11f7a0519ed53bb6b7abf9","tid":1703627,"uid":315724,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-10T18:10:56.000Z","msg":"Push"},{"pid":"d6c254b18dea975baa19487406cdba02fdc2c57e","tid":1703627,"uid":311238,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-10T18:24:58.000Z","msg":"P牌仔推唔到<img src=\"/assets/faces/normal/frown.gif\" class=\"hkgmoji\" />"}]}