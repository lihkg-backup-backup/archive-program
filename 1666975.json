/a>"},{"pid":"6a2e1e80688fff0d68fac4093fab98a09b91e486","tid":1666975,"uid":227275,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-10-21T08:32:10.000Z","msg":"1"},{"pid":"7bc405993ade9ac99cae45fbc5fe75bb5a065b89","tid":1666975,"uid":227275,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-10-21T09:44:40.000Z","msg":"1"},{"pid":"b3ddb37a00113ace6f3f69470a63850630d65714","tid":1666975,"uid":216228,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-10-21T09:49:53.000Z","msg":"所以話，<br />\n用屎攻對佢哋冇用。<br />\n本身已經臭過屎<img src=\"/assets/faces/dog/haha.gif\" class=\"hkgmoji\" />"}]}