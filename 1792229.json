，新上就狂派飛，手足成功收黃百嗚條藍狗皮"},{"pid":"6fb0c73b0e56370d20c47d143bbfc8f2e491bea9","tid":1792229,"uid":205440,"like":0,"dislike":0,"score":0,"quote":{"pid":"164d21fa1a98aa1fa94dba9269199eb26ef58940","tid":1792229,"uid":40867,"like":0,"dislike":4,"score":-4,"citedBy":1,"replyTime":"2019-12-21T13:10:30.000Z","msg":"請支持知識產權<br />\n<br />\n不要推"},"citedBy":1,"replyTime":"2019-12-21T13:44:20.000Z","msg":"不要推<br />\n點用不推文回覆？"},{"pid":"7186fad8185be784859719e1bc6c2d9e9becebb9","tid":1792229,"uid":40867,"like":0,"dislike":0,"score":0,"quote":{"pid":"6fb0c73b0e56370d20c47d143bbfc8f2e491bea9","tid":1792229,"uid":205440,"like":0,"dislike":0,"score":0,"quote":{"pid":"164d21fa1a98aa1fa94dba9269199eb26ef58940","tid":1792229,"uid":40867,"like":0,"dislike":4,"score":-4,"citedBy":1,"replyTime":"2019-12-21T13:10:30.000Z","msg":"請支持知識產權<br />\n<br />\n不要推"},"citedBy":1,"replyTime":"2019-12-21T13:44:20.000Z","msg":"不要推<br />\n點用不推文回覆？"},"citedBy":1,"replyTime":"2019-12-21T13:44:57.000Z","msg":"不知道呢<br />\n<br />\n反正不要推"},{"pid":"25c9043803c03ef806f0e48634051fc02a695a66","tid":1792229,"uid":40867,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-12-21T13:45:07.000Z","msg":"千萬不要繼續推"},{"pid":"32e1a9c0ba46048e3f6ffeb1ab2f4e0990789e87","tid":1792229,"uid":1888,"like":1,"dislike":0,"score":1,"citedBy":1,"replyTime":"2019-12-21T13:52:11.000Z","msg":"<img src=\"/assets/faces/normal/dislike.gif\" class=\"hkgmoji\" />"}]}