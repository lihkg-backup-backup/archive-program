g src=\"/assets/faces/normal/frown.gif\" class=\"hkgmoji\" />"},{"pid":"c0da71093b4e5c8e866d2c7c0bb7c46205129cca","tid":1723516,"uid":24570,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-16T05:07:30.000Z","msg":"人地唔肯罷都冇用"},{"pid":"c3b7da82707709559c601e639ee544cb595d6dc0","tid":1723516,"uid":70443,"like":0,"dislike":0,"score":0,"citedBy":0,"replyTime":"2019-11-16T05:11:02.000Z","msg":"Tui"},{"pid":"de18cf49f0cb0177f07f406645c8df003ff28c1e","tid":1723516,"uid":19164,"like":0,"dislike":0,"score":0,"quote":{"pid":"c88131da6a343e9e441c93a1374c690e66d43413","tid":1723516,"uid":19164,"like":0,"dislike":0,"score":0,"citedBy":1,"replyTime":"2019-11-15T14:36:18.000Z","msg":"推(6/20）"},"citedBy":1,"replyTime":"2019-11-16T10:02:52.000Z","msg":"推（7/20）"}]}